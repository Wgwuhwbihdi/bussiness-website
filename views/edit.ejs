        <input type="text" value="<%=edit1.websitetype%>" name="websitedata[websitetype]" placeholder="websitetype" /><br />
        <label for="">enter the discription which you want to add</label>
        <input type="text" value="<%=edit1.description%>" name="websitedata[description]" placeholder="brif discription" /><br />
